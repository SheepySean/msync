{
  "name": "msync",
  "version": "2.1.5",
  "description": "Easily manage building and syncing multiple node-modules in a flexibly defined workspace.",
  "main": "./lib/index",
  "types": "./lib/index.d.ts",
  "bin": {
    "msync": "./bin-cli"
  },
  "scripts": {
    "test": "sh ./sh/test.sh",
    "tdd": "yarn run test --reporter min --watch",
    "lint": "sh ./sh/lint.sh",
    "build__": "sh ./sh/build.sh",
    "build": "ts build $@",
    "prepare": "yarn run lint && yarn test && yarn run build"
  },
  "dependencies": {
    "command-interface": "4.0.7",
    "rsync": "0.6.1",
    "semver": "5.6.0",
    "toposort": "2.0.2"
  },
  "devDependencies": {
    "@platform/test": "0.0.27",
    "@platform/ts.cli": "3.3.42",
    "@types/rsync": "0.4.30",
    "@types/semver": "5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/msync"
  },
  "license": "MIT"
}
