{
  "name": "module-sync",
  "version": "0.0.1",
  "description": "Manages building and syncing multiple node modules.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "bin": {
    "sm": "./bin-cli"
  },
  "scripts": {
    "test": "sh sh/test.sh",
    "tdd": "yarn run test -- --reporter min --watch",
    "lint": "tslint 'src/**/*.ts{,x}' --format verbose",
    "build": "rm -rf ./lib && tsc",
    "build:watch": "yarn run build -- --watch",
    "prepublish": "yarn run lint && yarn test && yarn run build"
  },
  "dependencies": {
    "command-interface": "^2.4.1",
    "fs-extra-promise": "^1.0.1",
    "glob": "^7.1.1",
    "js-util-log": "^1.0.4",
    "js-yaml": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/fs-extra-promise": "^1.0.0",
    "@types/glob": "^5.0.30",
    "@types/js-yaml": "^3.5.30"
    "@types/mocha": "^2.2.41",
    "chai": "^3.5.0",
    "js-typescript": "^2.3.1",
    "mocha": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/module-sync"
  },
  "license": "MIT"
}
